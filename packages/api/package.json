    "@types/node": "^22.10.6",
    "@types/passport": "^1.0.4",
    "@types/passport-strategy": "^0.2.35",
    "@types/sharp": "^0.30.4",
    "@types/supertest": "2.0.12",
    "@types/ungap__structured-clone": "^0.3.0",
    "@types/unist": "^3.0.3",
