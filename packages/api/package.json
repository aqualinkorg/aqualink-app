    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^5.7.3",
    "yargs": "^17.7.2"
  },
  "_optionalDependencies": {
