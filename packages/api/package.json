    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^8.0.8",
    "@nestjs/typeorm": "^10.0.2",
    "@turf/turf": "^7.3.1",
    "@types/bluebird": "^3.5.32",
    "axios": "^1.12.2",
    "axios-retry": "^3.8.1",
    "bluebird": "^3.7.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "csv-parse": "^4.15.1",
    "csv-parser": "^3.2.0",
    "csv-stringify": "^6.4.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.0",
    "express": "^4.21.2",
    "firebase-admin": "^13.0.2",
    "geo-tz": "^6.0.0",
    "geojson": "^0.5.0",
    "geotiff": "^1.0.4",
    "hashids": "^2.2.1",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "md5-file": "^5.0.0",
    "multer": "^2.0.2",
    "node-gyp": "^11.4.2",
    "node-xlsx": "^0.24.0",
    "objects-to-csv": "^1.3.6",
    "p-limit": "^7.2.0",
    "passport": "^0.6.0",
    "passport-custom": "^1.1.1",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7",
    "sharp": "^0.33.5",
    "ts-exif-parser": "^0.2.1",
    "typeorm": "^0.3.20",
