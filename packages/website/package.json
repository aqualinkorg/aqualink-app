{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env EXTEND_ESLINT=true GENERATE_SOURCEMAP=false react-scripts start",
    "build": "cross-env EXTEND_ESLINT=true GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts --expose-gc test --logHeapUsage --maxWorkers=4",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src --ignore-path .gitignore --max-warnings 0",
    "precommit": "lint-staged",
    "deploy:prod": "dotenv -e .env.prod yarn build && yarn run firebase deploy --only hosting:prod-target",
    "deploy:staging": "dotenv -e .env.staging yarn build && yarn run firebase deploy --only hosting:staging-target",
    "deploy:programize": "dotenv -e .env.programize yarn build && yarn run firebase deploy --only hosting:programize-target"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.3.11",
    "@reduxjs/toolkit": "^1.3",
    "@sketchfab/viewer-api": "^1.12.0",
    "@testing-library/dom": "^7.8.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^1.6.0",
    "chart.js": "^2.9.3",
    "chartjs-plugin-annotation": "^0.5.7",
    "classnames": "^2.2.6",
    "date-fns-tz": "^1.1.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "firebase": "^10.2.0",
    "firebase-tools": "^11.29.1",
    "immutable": "^4.0.0-rc.12",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.15",
    "luxon": "^3.3.0",
    "mutationobserver-shim": "^0.3.7",
    "notistack": "^1.0.10",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.9.0",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.3",
    "react-ga4": "^2.1.0",
    "react-hook-form": "^7.41.5",
    "react-jwt": "^1.1.8",
    "react-leaflet": "^2.7.0",
    "react-leaflet-markercluster": "^2.0.0",
    "react-material-ui-carousel": "^1.8.2",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-slick": "^0.27.10",
    "react-swipeable-bottom-sheet": "^1.1.2",
    "react-swipeable-views": "^0.13.9",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "slick-carousel": "^1.8.1",
    "validator": "^13.1.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    },
    "globalSetup": "./src/global-setup.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "18.x"
  },
  "devDependencies": {
    "@testing-library/react": "^9.5.0",
    "@types/chart.js": "^2.9.27",
    "@types/classnames": "^2.2.10",
    "@types/immutable": "^3.8.7",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.149",
    "@types/luxon": "^3.3.1",
    "@types/node": "^18.16.16",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-leaflet": "^2.5.1",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-slick": "^0.23.4",
    "@types/react-swipeable-views": "^0.13.0",
    "@types/redux-mock-store": "^1.0.2",
    "@types/validator": "^13.1.0",
    "canvas": "^2.11.2",
    "cross-env": "^7.0.2",
    "dotenv-cli": "^4.0.0",
    "jest-date-mock": "^1.0.8",
    "react-scripts": "5.0.1",
    "surge": "^0.23.1"
  }
}
