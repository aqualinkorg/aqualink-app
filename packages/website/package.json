    "eslint": "^9.0.0",
    "firebase": "^11.0.0",
    "firebase-tools": "^13.31.0",
    "geolib": "^3.3.4",
    "hono": "^4.7.2",
    "immutable": "^4.0.0-rc.12",
    "leaflet": "^1.9.4",
    "leaflet.locatecontrol": "^0.79.0",
    "lodash": "^4.17.15",
