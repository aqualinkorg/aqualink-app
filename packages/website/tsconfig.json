{
  "extends": "../../tsconfig.json",
  "include": [
    "src",
    "./build/types/**/*.ts",
    ".next/types/**/*.ts"
  ],
  "files": [
    "src/custom.d.ts",
    "./next-env.d.ts"
  ],
  "compilerOptions": {
    "noFallthroughCasesInSwitch": true,
    "baseUrl": "./src",
    "incremental": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
